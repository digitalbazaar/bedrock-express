{
  "name": "bedrock-express-test",
  "version": "0.0.1-0",
  "private": true,
  "scripts": {
    "test": "node --preserve-symlinks test test",
    "coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary npm test",
    "coverage-ci": "cross-env NODE_ENV=test nyc --reporter=lcovonly npm test",
    "coverage-report": "nyc report"
  },
  "dependencies": {
    "@digitalbazaar/http-client": "^2.0.1",
    "bedrock": "^4.4.0",
    "bedrock-express": "file:..",
    "bedrock-https-agent": "^2.1.0",
    "bedrock-server": "^3.0.0",
    "bedrock-test": "^5.4.0",
    "cross-env": "^7.0.2",
    "nyc": "^15.1.0"
  },
  "nyc": {
    "excludeNodeModules": false,
    "include": [
      "node_modules/bedrock-express/**"
    ],
    "exclude": [
      "node_modules/bedrock-express/node_modules/**"
    ]
  }
}
